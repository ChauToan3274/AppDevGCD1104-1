@model List<Category>
<h1>Here is a list of Category</h1>
<table class="table table-bordered">
	<thead>
		<tr>
			<td>Name</td>
			<td>Description</td>
			<td>Display Order</td>
		</tr>
	</thead>
	<tbody>
		@foreach (var category in Model)
		{
			<tr>
				<td>@category.Name</td>
				<td>@category.Description</td>
				<td>@category.DisplayOrder</td>
			</tr>
		}
	</tbody>
	
</table>